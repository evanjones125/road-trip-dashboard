{
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "buildCommand": "pip install -r requirements.txt -t ./python"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/wsgi.py"
    }
  ]
}